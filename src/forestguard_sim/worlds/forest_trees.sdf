    <include><uri>model://tree1</uri><name>tree1_12</name><pose>-5.0 -3.2 0 0 0 0</pose></include>
    <include><uri>model://tree1</uri><name>tree1_13</name><pose>-1.7 -3.2 0 0 0 0</pose></include>
    <include><uri>model://tree1</uri><name>tree1_14</name><pose>1.4 -3.2 0 0 0 0</pose></include>
