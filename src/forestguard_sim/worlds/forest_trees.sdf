    <include><uri>model://tree1</uri><name>tree1_7</name><pose>-3.3 -6.0 0 0 0 0</pose></include>
    <include><uri>model://tree1</uri><name>tree1_8</name><pose>0.2 -6.0 0 0 0 0</pose></include>
